n = 11
record = ""
f = ""
size = 0
generation_num = 60

while n <= 40
  FILE_FINISH = "false"
  out_record = ""
  dname = "generation" + generation_num + "/"
  fname = dname + "log" + FORMAT(n, 2) + ".txt"

  fileID = FOPEN(fname, F_READ) // 読み込みモードでファイルを開く
    record = FGET(fileID, 1)
  FCLOSE(fileID)

  if size < length(record)
    size = length(record)
    f = fname
  ENDIF

  n = n + 1
wend

print (f)
